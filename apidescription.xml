    <!-- Discovery REST API Version 2 -->

    <!-- Search Events -->
    <resource path="discovery/v2/events.{format}">
      <param name="format" required="true" type="xsd:string" style="template" default="json">
        <doc>API Response Format</doc>
      </param>
      <param name="keyword" required="false" style="query" type="xsd:string">
        <doc>A string to search against events, attractions and venues. The keyword will be checked against titles, descriptions, names and other logical fields that describe any of these data objects.</doc>
      </param>
      <param name="attractionId" required="false" style="query" type="xsd:string" default="“K8vZ91713eV”">
        <doc>Attraction ID(s) separated by comma.</doc>
      </param>
      <param name="venueId" required="false" style="query" type="xsd:string" default="“KovZpZAEdFtJ”">
        <doc>Venue ID(s) separated by comma.</doc>
      </param>
      <param name="promoterId" required="false" style="query" type="xsd:string" default="494">
        <doc>Promoter ID(s) separated by comma.</doc>
      </param>
      <param name="postalCode" required="false" style="query" type="xsd:string" default="90069">
        <doc>Zipcode or Postal Code of the venue in which the event is taking place.</doc>
      </param>
      <param name="latlong" required="false" style="query" type="xsd:string" default="34.0928090,-118.3286610">
        <doc>The Latitude,Longitude coordinates for the venue in which this event is taking place.</doc>
      </param>
      <param name="radius" required="false" style="query" type="xsd:integer" default="25">
        <doc>The radius of the area in which we want to search for events.</doc>
      </param>
      <param name="unit" required="false" style="query" type="xsd:string" default="miles">
        <doc>The radius distance unit. Possible values: miles, km.</doc>
      </param>
      <param name="source" required="false" style="query" type="xsd:string" default="">
        <doc>Source of the event. Possible values are '', 'ticketmaster', 'ticketweb', 'universe'.</doc>
      </param>
      <param name="locale" required="false" type="xsd:string" style="query">
        <doc>The event locale, including country and localization.</doc>
        <option value=""/>
        <option value="en-us"/>
        <option value="en-gb"/>
        <option value="en-ca"/>
        <option value="es-us"/>
        <option value="en-mx"/>
        <option value="es-mx"/>
        <option value="en-au"/>
        <option value="en-nz"/>
        <option value="fr-fr"/>
        <option value="fr-ca"/>
      </param>
      <param name="marketId" required="false" style="query" type="xsd:integer" default="27">
        <doc>The city/area in which this event takes place.</doc>
      </param>
      <param name="startDateTime" required="false" style="query" type="xsd:integer" default="1">
        <doc>Include events happening after this date.</doc>
      </param>
      <param name="endDateTime" required="false" style="query" type="xsd:integer" default="1">
        <doc>Include events happening before this date.</doc>
      </param>
      <param name="includeTBA" required="false" style="query" type="xsd:string">
        <doc>Whether or not to return events with dates to be announced (TBA). Default is 'no', TBA events are not returned.</doc>
        <option value=""/>
        <option value="yes"/>
        <option value="no"/>
        <option value="only"/>
      </param>
      <param name="includeTBD" required="false" style="query" type="xsd:string">
        <doc>Whether or not to return events with dates to be announced (TBD). Default is 'no', TBD events are not returned.</doc>
        <option value=""/>
        <option value="yes"/>
        <option value="no"/>
        <option value="only"/>
      </param>
      <param name="includeTest" required="false" style="query" type="xsd:string">
        <doc>Whether or not to return test events. Default is 'no', test events are not returned.</doc>
        <option value=""/>
        <option value="yes"/>
        <option value="no"/>
        <option value="only"/>
      </param>
      <param name="size" required="false" style="query" type="xsd:integer" default="20">
        <doc>The number of events returned in the API response.</doc>
      </param>
      <param name="page" required="false" style="query" type="xsd:integer" default="1">
        <doc>The page for paginating through the results.</doc>
      </param>
      <param name="sort" required="false" style="query" type="xsd:string">
        <doc>The search sort criteria.</doc>
        <option value=""/>
        <option value="name,desc"/>
        <option value="name,asc"/>
        <option value="date,asc"/>
        <option value="date,desc"/>
      </param>
      <param name="city" required="false" style="query" type="xsd:string">
        <doc>The city.</doc>
      </param>
      <param name="countryCode" required="false" style="query" type="xsd:string">
        <doc>The country code.</doc>
      </param>
      <param name="stateCode" required="false" style="query" type="xsd:string">
        <doc>The state code.</doc>
      </param>
      <param name="segmentId" required="false" style="query" type="xsd:string">
        <doc>The segmentId.</doc>
      </param>
      <param name="segmentName" required="false" style="query" type="xsd:string">
        <doc>The segmentName.</doc>
      </param>
      <param name="dmaId" required="false" style="query" type="xsd:string">
        <doc>The dmaId.</doc>
      </param>
      <method id="discovery.v2.events.get" name="GET" apigee:displayName="Search Events">
        <apigee:tags>
          <apigee:tag primary="true">Discovery API v2</apigee:tag>
          <apigee:tag>Events</apigee:tag>
        </apigee:tags>
        <apigee:authentication required="true"/>
        <apigee:example url="discovery/v2/events.json"/>
        <doc apigee:url="http://developer.ticketmaster.com/products-and-docs/apis/discovery/#srch-events">Returns the 20 most recent events for the authenticating user.</doc>
      </method>
    </resource>

    <!-- Event Details -->
    <resource path="discovery/v2/events/{id}.{format}">
      <param name="id" required="true" type="xsd:string" style="template" default="G5diZfkn0B-bh">
        <doc>Event ID</doc>
      </param>
      <param name="format" required="true" type="xsd:string" style="template" default="json">
